node{
    stage('sample'){
        checkout(scm)
        def repositoryUrl = scm.userRemoteConfigs[0].url
        echo "RepoURL - ${repositoryUrl}"
        //git 'https://github.com/srinivasarao2468/jenkins_check.git'
        properties([buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '7', numToKeepStr: '7'))])
        def elements =  repositoryUrl.split("/")
        def projectName = elements[elements.size()-1].split("\\.")[0]
        echo projectName
        def app = projectName.split("\\-")
        siz = app.size()
        def project_name = (app[siz-2]+"-"+app_name).toLowerCase()
        echo project_name
        echo app[siz-2]
        def app_name = app[siz-1].toLowerCase()
        def date = new Date()
        println date.format("YYYYMMddHHmmss")
    }
}
